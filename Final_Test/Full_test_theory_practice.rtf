import maya.OpenMaya as OpenMaya\par
import maya.cmds as cmds\par
import random\par
\par
selection = cmds.ls(sl=1)\par
selected_object = selection[0]\par
other_objs = selection[1:]\par
\par
selection_list = OpenMaya.MSelectionList()\par
selection_list.add(selected_object)\par
mDagPath = OpenMaya.MDagPath()\par
selection_list.getDagPath(0, mDagPath)\par
\par
vertex_iterator = OpenMaya.MItMeshVertex(mDagPath)\par
\par
while not vertex_iterator.isDone():\par
    index = vertex_iterator.index()\par
    coordinates = vertex_iterator.position()\par
   \par
    for obj in other_objs:\par
        x = random.uniform(0, coordinates.x)\par
        y = random.uniform(0, coordinates.y)\par
        z = random.uniform(0, coordinates.z)\par
        cmds.xform(obj, t=(x, y, z))\par
\par
    vertex_iterator.next()\par
